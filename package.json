{
  "name": "mpyc-web-all",
  "version": "0.6.0",
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "workspaces": [
    "mpyc-web-core",
    "mpyc-web-demo",
    "mpyc-web-peerjs"
  ],
  "scripts": {
    "dev": "yarn workspace @mpyc-web/demo dev",
    "build:py": "poetry build -f wheel && cp ./dist/*.whl ./mpyc-web-demo/public/py && cp dist/*.whl ./mpyc-web-demo/dist/py/",
    "build": "yarn workspace @mpyc-web/core build && yarn workspace @mpyc-web/demo build",
    "preview": "vite preview",
    "static": "python -m http.server -d dist",
    "ghp": "yarn build && yarn gh-pages -d mpyc-web-demo/dist -t true",
    "gh-pages": "APP_DEPLOYMENT='' yarn ghp",
    "gh-pages-test": "APP_DEPLOYMENT='test' yarn ghp --remote test",
    "gh-pages-mpyc": "APP_DEPLOYMENT='' yarn ghp --remote mpyc",
    "clean": "rm -rf ./*/dist; rm -rf ./*/node_modules; rm -rf ./*/vite.config.ts.timestamp-*.mjs"
  },
  "devDependencies": {
    "@types/gh-pages": "^5.0.0",
    "@types/git-rev-sync": "^2.0.1",
    "coi-serviceworker": "^0.1.7",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.0.0",
    "git-rev-sync": "^3.0.2",
    "node-sass": "^9.0.0",
    "pyodide": "^0.24.1",
    "rollup": "^4.1.5",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "sass": "^1.69.5",
    "tslib": "^2.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-externalize-dependencies": "^0.10.0"
  }
}
