{
  "name": "mpyc-web-all",
  "version": "0.8.0",
  "type": "module",
  "workspaces": [
    "mpyc-web-core",
    "mpyc-web-demo",
    "mpyc-web-demo-standalone",
    "mpyc-web-peerjs"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace @mpyc-web/demo dev\" \"yarn vite --debug\"",
    "bun:dev": "concurrently \"cd mpyc-web-demo && bun dev\" \"bun vite --debug\"",
    "build:py": "poetry build -f wheel && cp ./dist/*.whl ./mpyc-web-demo/public/py && cp dist/*.whl ./mpyc-web-demo/dist/py/",
    "build": "yarn build:py && yarn workspace @mpyc-web/core build && yarn workspace @mpyc-web/demo build",
    "preview": "vite preview",
    "static": "python -m http.server -d dist",
    "ghp": "yarn build && gh-pages -d mpyc-web-demo/dist -t true",
    "gh-pages-web": "APP_DEPLOYMENT='' yarn ghp",
    "gh-pages-test": "APP_DEPLOYMENT='test' yarn ghp --remote test",
    "gh-pages-mpyc": "APP_DEPLOYMENT='' bun ghp --remote mpyc",
    "clean": "rm -rf ./*/dist; rm -rf ./*/node_modules; rm -rf ./*/vite.config.ts.timestamp-*.mjs"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/gh-pages": "^5.0.1",
    "@types/git-rev-sync": "^2.0.2",
    "coi-serviceworker": "^0.1.7",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.0",
    "git-rev-sync": "^3.0.2",
    "node-sass": "^9.0.0",
    "pyodide": "^0.24.1",
    "rollup": "^4.6.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "sass": "^1.69.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.3",
    "vite-plugin-externalize-dependencies": "^0.11.0"
  },
  "dependencies": {
    "@lezer/common": "^1.1.1",
    "@lezer/highlight": "^1.2.0",
    "coincident": "^1.0.1",
    "emittery": "^1.0.1",
    "events": "^3.3.0",
    "vite-plugin-run": "^0.5.1"
  },
  "packageManager": "yarn@4.0.2"
}
