{
  "name": "@mpyc-web/demo-standalone",
  "version": "0.8.0",
  "type": "module",
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "dev": "APP_DEPLOYMENT=dev vite --debug",
    "build:py": "mkdir -p ./dist/py ./public/py && poetry build -f wheel && cp ../dist/*.whl ./public/py/ && cp ../dist/*.whl ./dist/py/",
    "build": "tsc && bun build:py && vite build",
    "preview": "vite preview",
    "static": "python -m http.server",
    "ghp": "gh-pages -d dist -t true",
    "gh-pages": "APP_DEPLOYMENT='' bun build && gh-pages -d dist -t true",
    "gh-pages-test": "APP_DEPLOYMENT='test' bun build && gh-pages -d dist -t true --remote test",
    "gh-pages-mpyc": "APP_DEPLOYMENT='' bun build && gh-pages -d dist -t true --remote mpyc"
  },
  "devDependencies": {
    "@pyodide/webpack-plugin": "^1.3.2",
    "@types/gh-pages": "^5.0.1",
    "@types/git-rev-sync": "^2.0.2",
    "@types/glob": "^8.1.0",
    "coi-serviceworker": "^0.1.7",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.1.0",
    "git-rev-sync": "^3.0.2",
    "glob": "^10.3.10",
    "rollup": "^4.6.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.3",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-externalize-dependencies": "^0.11.0",
    "vite-plugin-run": "^0.5.1",
    "vite-plugin-top-level-await": "^1.3.1"
  },
  "files": [
    "lib/**/*.whl",
    "lib/**/*.py"
  ]
}
