[MESSAGES CONTROL]
disable=abstract-method,missing-module-docstring,missing-class-docstring,unused-argument,unused-import,logging-fstring-interpolation,single-string-used-for-slots,unnecessary-lambda-assignment,line-too-long,missing-function-docstring,await-outside-async

[BASIC]
argument-naming-style=any
attr-naming-style=any
variable-naming-style=any
const-naming-style=any
function-naming-style=any
method-naming-style=any

[DESIGN]
# Maximum number of arguments for function / method.
max-args=6
